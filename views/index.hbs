        {{!-- project 2 --}}
        {{#each projects}}
        <div style="width: 295px; margin-left: 40px">
          <div style="margin-top: 100px">
            <div class="blog-detail">
              <div>
                <img
                  src="/uploads/{{this.image}}"
                  class="style-imageBlogDetail"
                  alt="image project"
                />
              </div>
              <div>
                <div>
                  <h5 style="margin-top: 10px">{{this.name}}</h5>
                  <p style="color: gray; margin-bottom: 10px">{{this.start_date}} | {{this.end_date}}</p>
                </div>
                <p style="color: gray; text-align: right; margin-right: 20px">{{this.author}}</p>
                <div>
                  <p>
                    {{this.content}}
                  </p>
                  <a
                    href="/project-detail/{{this.id}}" target="_blank"
                    style="color: rgb(152, 155, 95); text-decoration: none"
                    ><p
                      style="
                        font-size: 13px;
                        margin-bottom: 17px;
                        margin-top: 10px;
                      "
                    >
                      click here for detail
                    </p></a
                  >
                  <div>
                    <span
                      style="margin-left: 200px; color: gray; font-size: 10px"
                      >{{this.duration}}</span
                    >
                  </div>
                </div>
              </div>
              <div class="style-icons">
                <div>
                  {{#if nodeJs}}
                  <i class="bx bxl-nodejs"></i>
                  {{/if}}
                </div>
                <div>
                 {{#if javascripts}}
                  <i class="bx bxl-javascript"></i>
                  {{/if}}
                </div>
                <div>
                  {{#if reactJs}}
                  <i class="bx bxl-react"></i>
                  {{/if}}
                </div>
                <div>
                  {{#if vueJs}}
                  <i class="bx bxl-vuejs"></i>
                  {{/if}}
                </div>
              </div>
              <div class="style-buttonBlogDetails">
                <div>
                  <button class="style-buttonEdit"><a href="/update-project/{{this.id}}" style="text-decoration: none; color: white">edit</a></button>
                </div>
                <div>
                  <button class="style-buttonDelete"><a href="/delete-project/{{this.id}}" style="text-decoration: none; color: white">delete</a></button>
                </div>
              </div>
            </div>
          </div>
        </div>
        {{/each}}
